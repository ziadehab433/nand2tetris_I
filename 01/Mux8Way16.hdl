CHIP Mux8Way16 {
    IN a[16], b[16], c[16], d[16],
       e[16], f[16], g[16], h[16],
       sel[3];
    OUT out[16];

    PARTS:
    Mux16(a=a, b=b, sel=sel[0], out=i);
    Mux16(a=c, b=d, sel=sel[0], out=o);
    Mux16(a=i, b=o, sel=sel[1], out=fb);
    Mux16(a=e, b=f, sel=sel[0], out=r);
    Mux16(a=g, b=h, sel=sel[0], out=t);
    Mux16(a=r, b=t, sel=sel[1], out=sb);
    Mux16(a=fb, b=sb, sel=sel[2], out=out);
}
