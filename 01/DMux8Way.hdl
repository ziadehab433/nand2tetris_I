CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    DMux(in=in, sel=sel[2], a=q, b=w);
    DMux(in=q, sel=sel[1], a=j, b=k);
    DMux(in=j, sel=sel[0], a=a, b=b);
    DMux(in=k, sel=sel[0], a=c, b=d);
    DMux(in=w, sel=sel[1], a=i, b=o);
    DMux(in=i, sel=sel[0], a=e, b=f);
    DMux(in=o, sel=sel[0], a=g, b=h);
}
